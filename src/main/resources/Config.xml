                <ref bean="9C"/>
            </list>
        </constructor-arg>
        <constructor-arg name="gameOver" value="false"/>
        <constructor-arg name="roundOver" value="true"/>


    </bean>


    <bean id="TestPlayer1" class="com.example.COMP4004A3.Game.Player">
