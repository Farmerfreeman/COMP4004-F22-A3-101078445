    <bean id = "TestGameState" class="com.example.COMP4004A3.Models.GameState">
        <constructor-arg name ="playerTurn" value = "1"/>
        <constructor-arg name = "players">
            <list>
                <ref bean="TestPlayer1"/>
                <ref bean="TestPlayer2"/>
                <ref bean="TestPlayer3"/>
                <ref bean="TestPlayer4"/>
            </list>
        </constructor-arg>
        <constructor-arg name="stockPile">
